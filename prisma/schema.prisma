// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model Users {
  id   Int    @id @default(autoincrement())
  UIN  String @unique
  GBM1 Int    @default(0)
  GBM2 Int    @default(0)
}
